#include "funciones.h"

int DarDeAlta(eEmpleado lista[], int tamanio) {

	int i;
	int retorno;
	retorno = 0;

	for (i = 0; i <= tamanio; i++) {

		if (lista[i].estavacio != 1) {

			lista[i].id = i;

			PedirNombre("ingrese el nombre del empleado: ", lista);
			PedirApellido("ingrese el apellido del empleado", lista);
			Salario("ingrese el salario del empleado", sueldo)
		}

	}

	return retorno;
}
